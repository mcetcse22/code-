public class NoGate {
    public static void main(String args[]) {
        int a[] = {1, 0, 1, 0, 1};
        int i, ans;

        for (i = 0; i < 5; i++) {
            if (a[i] == 0)
                ans = 1;
            else
                ans = 0;

            System.out.printf("NOT %d = %d\n", a[i], ans);
        }
    }
}
